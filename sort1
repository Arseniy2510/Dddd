"""
from random import randint
A = [randint(100,999) for i in range(3)]
print("Массив A:")
print(*A)
def zxc(x):
        while x>0:
            summ = 0
            summ+=x%10
            x//10
            return(summ)
for i in range(len(A)):
    swapped = False
    for j in range(0,len(A)-i-1):
        if zxc(A[j]) < zxc(A[j+1]):
            A[j], A[j + 1] = A[j + 1], A[j]
            swapped = True
    if not swapped:
        break
print("После сортировки:")
print(*A)

